         <xsd:documentation>A forensic dental and oral record detail</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ForensicDentalSetting" type="biom:ForensicDentalSettingType" nillable="true">
      <xsd:annotation>
         <xsd:documentation>A set of information regarding the forensic setting information that
            carried out the analysis of the dental and oral data to identify or confirm the identity
            of the subject.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ForensicInvestigatoryVoiceDetail"
      type="biom:ForensicInvestigatoryVoiceDetailType" nillable="true">
