      <xsd:annotation>
         <xsd:documentation>A country code, describing where a forensic analysis was
            performed.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ForensicSourceCountryGENCNumericCode" type="genc:CountryNumericCodeType"
