         <xsd:documentation>A data type for a representation of codecs capable of encoding or
            decoding a digital data stream or signal.</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:CodecCategoryCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
