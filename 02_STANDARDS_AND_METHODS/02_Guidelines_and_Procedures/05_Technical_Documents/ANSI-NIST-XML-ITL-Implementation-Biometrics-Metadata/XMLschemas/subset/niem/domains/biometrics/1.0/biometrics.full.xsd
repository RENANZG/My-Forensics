      </xsd:annotation>
   </xsd:element>
   <xsd:element name="FingerPattern" abstract="true" nillable="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a finger pattern. Deprecated.</xsd:documentation>
         <xsd:appinfo>
            <i:Deprecated i:value="true"/>
         </xsd:appinfo>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="FingerPatternCode" type="biom:FingerPatternCodeType"
      substitutionGroup="biom:FingerPattern" nillable="true">
