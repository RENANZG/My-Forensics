               <xsd:element ref="biom:LipPrintDescriptiveText" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:LipPrintComparisonDescriptiveText" minOccurs="0"
                  maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="CodecCategoryCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for an enumeration of codecs capable of encoding or decoding
            a digital data stream or signal.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="0">
            <xsd:annotation>
               <xsd:documentation>Linear PCM</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="1">
            <xsd:annotation>
               <xsd:documentation>Codec Type Reference</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="2">
            <xsd:annotation>
               <xsd:documentation>Other</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="3">
            <xsd:annotation>
               <xsd:documentation>Floating Point linear PCM</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="4">
            <xsd:annotation>
               <xsd:documentation>ITU-T G.711 (PCM): µ-law with forward order digital
                  samples</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="5">
            <xsd:annotation>
               <xsd:documentation>ITU-T G.711 (PCM): µ-law with reverse order digital
                  samples</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="6">
            <xsd:annotation>
               <xsd:documentation>ITU-T G.711 (PCM): A-law with forward order digital
                  samples</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="7">
            <xsd:annotation>
               <xsd:documentation>ITU-T G.711 (PCM): A-law with reverse order digital
                  samples</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
