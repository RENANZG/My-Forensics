      </xsd:annotation>
      <xsd:restriction base="xsd:nonNegativeInteger">
         <xsd:maxInclusive value="359"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="ThetaAngleMeasureType">
      <xsd:annotation>
         <xsd:documentation>A data type for an angular position of a feature from another point in
            an image</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:ThetaAngleMeasureSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:complexType name="TimeSegmentType">
      <xsd:annotation>
         <xsd:documentation>A data type for a time segment in a video or audio
            representation</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:TimeSegmentStartTimeValue" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:TimeSegmentEndTimeValue" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ToothDataADAReferenceCodeListType">
      <xsd:annotation>
         <xsd:documentation>A data type for a list of tooth data ADA reference valid codes from
            ANSI/ADA Standard No. 1058, Section 9</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:ADAReferenceCodeText" minOccurs="1" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ToothDataInformationType">
      <xsd:annotation>
         <xsd:documentation>A data type for a description of the transducer and transmission channel
            used to capture a recording.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:ToothDataRecordingDate" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:ToothDataRecordingDateEstimateRange" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:ToothID" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:OriginalSystemEncodingText" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:ToothDataADAReferenceCodeList" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:TransmittedToothEncodingText" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:ToothIDCertaintyCode" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:ToothDataAdditionalDescriptiveText" minOccurs="0"
                  maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="ToothIDCertaintyCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>The code value corresponding to the data set descriptors in Section 8 of
            the ANSI/ADA Standard No. 1058 may be entered.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="0">
            <xsd:annotation>
               <xsd:documentation>Unspecified</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="1">
            <xsd:annotation>
               <xsd:documentation>Certain</xsd:documentation>
