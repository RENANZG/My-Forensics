         </xsd:enumeration>
         <xsd:enumeration value="8">
            <xsd:annotation>
               <xsd:documentation>Livescan fingerprint swipe.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="10">
            <xsd:annotation>
               <xsd:appinfo><i:Deprecated i:value="true"/></xsd:appinfo>
               <xsd:documentation>Livescan palm. Deprecated.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="11">
