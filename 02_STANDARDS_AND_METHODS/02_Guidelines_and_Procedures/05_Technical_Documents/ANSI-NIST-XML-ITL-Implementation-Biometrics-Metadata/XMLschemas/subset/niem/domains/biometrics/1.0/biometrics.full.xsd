   </xsd:complexType>
   <xsd:complexType name="RecordingDurationType">
      <xsd:annotation>
         <xsd:documentation>A data type for a description of the duration of a recording, as
            expressed in time, size, and samplings.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:RecordingDurationTime" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:RecordingCompressedBytesQuantity" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:RecordingSampleQuantity" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="RecordingSettingDetailType">
      <xsd:annotation>
         <xsd:documentation>A data type for a description of the details surrounding the creation of
            a recording</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:RecordingSourceCategoryCode" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:SourceOrganizationName" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="nc:OrganizationPrimaryContactInformation" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:SourceCountryCode" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="RecordingSourceCategoryCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for the enumeration of codes describing the kind of site or
            agency that created a recording.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="G">
            <xsd:annotation>
               <xsd:documentation>Government</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="I">
            <xsd:annotation>
               <xsd:documentation>Industry/Commercial</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="O">
            <xsd:annotation>
               <xsd:documentation>Other laboratory</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="P">
            <xsd:annotation>
               <xsd:documentation>Private Individual</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="U">
            <xsd:annotation>
               <xsd:documentation>Unknown</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="RecordingSourceCategoryCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for the representation of codes describing the kind of site
            or agency that created a recording</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:RecordingSourceCategoryCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:simpleType name="RelativeRotationMeasureSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for a relative angle of rotation between -179 and 180, with
            0 being upright</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:integer">
         <xsd:minInclusive value="-179">
            <xsd:annotation>
               <xsd:documentation>A number identifying minimum relative angle of
                  rotation.</xsd:documentation>
            </xsd:annotation>
         </xsd:minInclusive>
         <xsd:maxInclusive value="180">
            <xsd:annotation>
               <xsd:documentation>A number identifying maximum relative angle of
                  rotation.</xsd:documentation>
            </xsd:annotation>
         </xsd:maxInclusive>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="RelativeRotationMeasureType">
      <xsd:annotation>
         <xsd:documentation/>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:RelativeRotationMeasureSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:complexType name="ResolutionMeansCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for an information item for means of determining
            resolution.</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:ResolutionMeansCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:simpleType name="ResolutionMeansCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for an information item for means of determining
            resolution.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="FLATBED">
            <xsd:annotation>
               <xsd:documentation>Resolution is known since the image was acquired from a flatbed
                  scanner with a fixed resolution</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="FIXED">
            <xsd:annotation>
               <xsd:documentation>Resolution is known since the image was acquired from a
                  fixed-resolution capture device other than a flatbed scanner</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="RULER">
            <xsd:annotation>
               <xsd:documentation>Resolution was calculated based upon a ruler present in the
                  image</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="FORM">
            <xsd:annotation>
               <xsd:documentation>Resolution was calculated based upon the use of a standard form
                  with a known scale</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="EST-HUMAN">
            <xsd:annotation>
               <xsd:documentation>Resolution was estimated by a human</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="EST-AUTO">
            <xsd:annotation>
               <xsd:documentation>Resolution was estimated by an automated
                  process.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="ResolutionMethodInformationType">
      <xsd:annotation>
         <xsd:documentation>A data type for the optional field that states the method used for
            determining the pixel density of the image.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:ResolutionMeansCode" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:ResolutionScaleLength" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:ResolutionScaleUnitCode" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:ResolutionScaleXCoordinateA" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:ResolutionScaleYCoordinateA" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:ResolutionScaleXCoordinateB" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:ResolutionScaleYCoordinateB" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:ResolutionCommentText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ResolutionScaleUnitCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a known scale units that indicates whether the known
            scales units are in inches or millimeters.</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:ResolutionScaleUnitCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:simpleType name="ResolutionScaleUnitCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for a known scale units that indicates whether the known
            scales units are in inches or millimeters.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="IN">
            <xsd:annotation>
               <xsd:documentation>inches</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="NM">
            <xsd:annotation>
               <xsd:documentation>millimeters</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="RestorationDataGranularityCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>This index indicates the type and level of restoration and surface
            information coded in Field 12.010: Tooth data detail / TDD.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="11">
            <xsd:annotation>
               <xsd:documentation>The system is capable of specifying individual restorations with
                  the restored surface information and material composition coded separately for
                  each restoration on the tooth; however, the submission of restorations with
                  materials specified for each restoration is optional. </xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="21">
            <xsd:annotation>
               <xsd:documentation>The system is capable of specifying individual restorations with
                  the restored surface coded separately; however, all of the individual material
                  compositions are combined into a single code for the tooth. Material specification
                  is optional. Unknown material composition may be implicitly or explicitly
                  coded.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="31">
            <xsd:annotation>
               <xsd:documentation>The system is capable of coding individual restorations with
                  restored surfaces into a single code. All the materials utilized in all the
                  restorations are combined into a single code when materials are represented. The
                  codes are specified by tooth.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="41">
            <xsd:annotation>
               <xsd:documentation>The presence of restorations without surface information is
                  combined to a single code for the tooth. All materials utilized in all the
                  restorations are combined into a single code for the tooth, when materials are
                  represented.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="51">
            <xsd:annotation>
               <xsd:documentation>Only the presence of restorations without surface or material
                  information is included in the coding.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="99">
            <xsd:annotation>
               <xsd:documentation>The level of detail contained in Field 12.010: Tooth data detail /
                  TDD concerning restorations, materials and/or surfaces is
                  unknown.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="RestorationDataGranularityCodeType">
      <xsd:simpleContent>
         <xsd:extension base="biom:RestorationDataGranularityCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:complexType name="RulerScalePresenceInformationType">
      <xsd:annotation>
         <xsd:documentation>A data type for the optional field allows the user to state whether a
            ruler or other known scale is present in the image.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:RulerScaleUnitCode" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:RulerScaleMakeText" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:RulerScaleModelText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="RulerScaleUnitCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for a unit of measurement visible on the ruler or
            measurement scale.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="IN">
            <xsd:annotation>
               <xsd:documentation>inches</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="MM">
            <xsd:annotation>
               <xsd:documentation>millimeters</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="BOTH">
            <xsd:annotation>
               <xsd:documentation>both inches and millimeters</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="RulerScaleUnitCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a unit of measurement visible on the ruler or
            measurement scale.</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:RulerScaleUnitCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>

   <!-- S -->
   <xsd:simpleType name="ScaleUnitsCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for a sampling frequency unit designator, pixels per inch,
            or pixels per centimeter</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:integer">
         <xsd:enumeration value="0">
            <xsd:annotation>
               <xsd:documentation>No scale given</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="1">
            <xsd:annotation>
               <xsd:documentation>Pixels per inch</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="2">
            <xsd:annotation>
               <xsd:documentation>Pixels per centimeter</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="ScaleUnitsCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a sampling frequency unit designator, pixels per inch,
            or pixels per centimeter</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:ScaleUnitsCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:complexType name="SegmentChannelInformationType">
      <xsd:annotation>
         <xsd:documentation>A data type for a description of the transducer and transmission channel
            used to capture a recording.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:SegmentIdentifierList" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:AudioCaptureDeviceCode" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:TransducerPrincipleCode" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:AudioCaptureEnvironmentText" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:TransducerDistanceValue" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:AcquisitionSourceCode" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:TransductionAlterationDescriptionText" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:ChannelCommentText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SegmentContentInformationType">
      <xsd:annotation>
         <xsd:documentation>A data type for an assessment of the content of the voice data within a
            list of identified time segments, including provision for semantic transcripts, phonetic
            transcriptions and translations of the segments.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:SegmentIdentifierList" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:SegmentTranscriptText" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:SegmentTranscriptLanguageCode" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:SegmentPhoneticTranscriptText" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:PhoneticTranscriptConvention" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:SegmentTranslationText" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:SegmentTranslationLanguageCode" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:SegmentContentCommentText" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:TranscriptAuthorityCommentText" minOccurs="0"
                  maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SegmentGeographicalInformationType">
      <xsd:annotation>
         <xsd:documentation>A data type for a list of specific geographical locations of the primary
            subject of a recording at the beginning of identified recording
            segments.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:SegmentGeographicalLocation" minOccurs="0"
                  maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SegmentGeographicalLocationType">
      <xsd:annotation>
         <xsd:documentation>A data type for a specific geographical location of the primary subject
            of a recording at the beginning of a recording segment.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:SegmentIdentifierList" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="nc:LocationDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="nc:LocationGeographicElevation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:LocationTwoDimensionalGeographicCoordinate" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="nc:LocationUTMCoordinate" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:LocationAlternateGeographicSystemValue" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:CellPhoneTowerCodeText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SegmentIdentifierListType">
      <xsd:annotation>
         <xsd:documentation>A data type for a list of specific time segment
            identifiers.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:SegmentIdentifier" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SegmentedImageType">
      <xsd:annotation>
         <xsd:documentation>A data type for an image representation that contains image
            segments</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="biom:ImageType">
            <xsd:sequence>
               <xsd:element ref="biom:ImageSegment" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="SegmentLocationCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for a finger segment location</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="PRX">
            <xsd:annotation>
               <xsd:documentation>Proximal Segment</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="DST">
            <xsd:annotation>
               <xsd:documentation>Distal Segment</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="MED">
            <xsd:annotation>
               <xsd:documentation>Medial Segment</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="NA">
            <xsd:annotation>
               <xsd:documentation>A full finger view, tip or the entire joint image
                  locations</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="UNK">
            <xsd:annotation>
               <xsd:documentation>The image is of an unknown segment of a finger or
                  thumb</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="SegmentLocationCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a finger segment location</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:SegmentLocationCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:complexType name="SegmentProcessingPriorityType">
      <xsd:annotation>
         <xsd:documentation>A data type for a list of identified segments, indicating the priority
            with which the segments should be processed.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:SegmentIdentifierList" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:ProcessingPriorityValue" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SegmentSpeakerCharacteristicsInformationType">
      <xsd:annotation>
         <xsd:documentation>A data type used to give an assessment of the characteristics of a voice
            within the segment, including intelligibility, emotional state and
            impairment.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:SegmentIdentifierList" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:SpeakerList" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:Type2CrossReferenceIdentificationList" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:ImpairmentValue" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:DominantSpokenLanguageCode" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:LanguageProficiencyValue" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:SpeechStyleCode" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:IntelligibilityValue" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:FamiliarityValue" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:HealthCommentText" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:EmotionalStateCode" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:VocalEffortValue" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:VocalStyleCode" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:RecordingAwarenessCode" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:ScriptText" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:SpeechCharacteristicsCommentText" minOccurs="0"
                  maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="SignatureCategoryCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for a kind of signature image, i.e. whether an image is the
            signature of the subject, or the signature of the official processing the
            transaction</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:integer">
         <xsd:enumeration value="0">
            <xsd:annotation>
               <xsd:documentation>Signature image of the subject</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="1">
            <xsd:annotation>
               <xsd:documentation>Signature image of the official processing the
                  transaction</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="SignatureCategoryCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a kind of signature image, i.e. whether an image is the
            signature of the subject, or the signature of the official processing the
            transaction</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:SignatureCategoryCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:complexType name="SignatureImageType">
      <xsd:annotation>
         <xsd:documentation>A data type for an image of a signature</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="biom:ImageType">
            <xsd:sequence>
               <xsd:element ref="biom:SignatureImageVectorRepresentation" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:SignatureRepresentationCode" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:SignatureCategoryCode" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SignatureImageVectorRepresentationType">
      <xsd:annotation>
         <xsd:documentation>A data type for a list of vectors that describe the pen position in a
            signature</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:SignatureImageVector" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SignatureImageVectorType">
      <xsd:annotation>
         <xsd:documentation>A data type for a vector that describes the pen position in a
            signature</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:VectorPenPressureValue" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:VectorPositionVerticalCoordinateValue" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:VectorPositionHorizontalCoordinateValue" minOccurs="0"
                  maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="SignatureRepresentationCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for an indicator whether an image is scanned and not
            compressed, scanned and compressed, or vector data</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:integer">
         <xsd:enumeration value="0">
            <xsd:annotation>
               <xsd:documentation>Scanned uncompressed</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="1">
            <xsd:annotation>
               <xsd:documentation>Scanned compressed</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="2">
            <xsd:annotation>
               <xsd:documentation>Vector</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="SignatureRepresentationCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for an indicator whether an image is scanned and not
            compressed, scanned and compressed, or vector data</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:SignatureRepresentationCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>   
   <xsd:complexType name="SourceAcquisitionType">
      <xsd:annotation>
         <xsd:documentation>A data type for an acquisition method for the source
            representation</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:AcquisitionSourceCode" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:AcquisitionDigitalConversionDescriptionText" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:AcquisitionFormatDescriptionText" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:AcquisitionRadioTransmissionFormatDescriptionText"
                  minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:AcquisitionSpecialCharacteristicsText" minOccurs="0"
                  maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SourceFileFormatType">
      <xsd:annotation>
         <xsd:documentation>A data type for a description of a file holding a source representation,
            and decoding instructions</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:SourceFileCategoryText" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:SourceFileDecodingInstructionsText" minOccurs="0"
                  maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="SourceRecordCardinalityCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for a cardinality of a source representation record relative
            to other records within the transaction</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="S">
            <xsd:annotation>
               <xsd:documentation>The representation in this Type-20 record is the source of another
                  Type-20 record</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="D">
            <xsd:annotation>
               <xsd:documentation>The representation in this Type-20 record is the source of one or
                  more biometric type records, excluding Type-4 and Type-9, which have been derived
                  from it</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="M">
            <xsd:annotation>
               <xsd:documentation>A single biometric type record, excluding Type-4 and Type-9, has
                  been prepared from multiple Type-20 records</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="SourceRecordCardinalityCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a cardinality of a source representation record relative
            to other records within the transaction</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:SourceRecordCardinalityCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:complexType name="SourceRepresentationType">
      <xsd:annotation>
         <xsd:documentation>A data type for a reference to a source
            representation</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:SourceIdentification" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:ImageSegmentIdentification" minOccurs="0"
                  maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SpeakerListType">
      <xsd:annotation>
         <xsd:documentation>A data type for a list of speaker identifiers.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:SpeakerIdentifier" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="SpeakerPluralityCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>It indicates plurality of speakers represented on voice
            recording.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="M">
            <xsd:annotation>
               <xsd:documentation>multiple speakers</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="S">
            <xsd:annotation>
               <xsd:documentation>single speaker</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="SpeakerPluralityCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a description of style of speech.</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:SpeakerPluralityCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:simpleType name="SpeechStyleCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for a description of style of speech.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:integer">
         <xsd:enumeration value="0">
            <xsd:annotation>
               <xsd:documentation>Unknown</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="1">
            <xsd:annotation>
               <xsd:documentation>Public speech (oratory)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="2">
            <xsd:annotation>
               <xsd:documentation>Conversation telephone </xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="3">
            <xsd:annotation>
               <xsd:documentation>Conversation face-to-face</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="4">
            <xsd:annotation>
               <xsd:documentation>Read</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="5">
            <xsd:annotation>
               <xsd:documentation>Prompted/repeated</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="6">
            <xsd:annotation>
               <xsd:documentation>Storytelling/Picture description</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="7">
            <xsd:annotation>
               <xsd:documentation>Task induced speech</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="8">
            <xsd:annotation>
               <xsd:documentation>Interview</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="9">
            <xsd:annotation>
               <xsd:documentation>Recited/memorized</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="10">
            <xsd:annotation>
               <xsd:documentation>Spontaneous/free</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="11">
            <xsd:annotation>
               <xsd:documentation>Variable</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="12">
            <xsd:annotation>
               <xsd:documentation>Other</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="SpeechStyleCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a description of style of speech.</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:SpeechStyleCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:simpleType name="SubjectBodyClassCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A classification of body tissue</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="1">
            <xsd:annotation>
               <xsd:documentation>Natural Tissue</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="2">
            <xsd:annotation>
               <xsd:documentation>Decomposed</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="3">
            <xsd:annotation>
               <xsd:documentation>Skeletal</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="SubjectBodyClassCodeType">
      <xsd:annotation>
         <xsd:documentation>A classification of body tissue</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:SubjectBodyClassCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:simpleType name="SubjectBodyStatusCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A status of a human body</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="1">
            <xsd:annotation>
               <xsd:documentation>Whole</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="2">
            <xsd:annotation>
               <xsd:documentation>Fragment</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="SubjectBodyStatusCodeType">
      <xsd:annotation>
         <xsd:documentation>A status of a human body</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:SubjectBodyStatusCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:simpleType name="SubjectDNARecordsAvailableCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>It contains a single letter for DNA records are available for the
            subject or not</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="1">
            <xsd:annotation>
               <xsd:documentation>Yes</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="2">
            <xsd:annotation>
               <xsd:documentation>No</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="SubjectDNARecordsAvailableCodeType">
      <xsd:simpleContent>
         <xsd:extension base="biom:SubjectDNARecordsAvailableCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:complexType name="SubjectExistentialDetailsType">
      <xsd:annotation>
         <xsd:documentation>This field is optional. This field is particularly useful if the image
            contained in this record is of an injury or latent image on a person. However, its use
            is not limited to such circumstances. Note that Field 10.041: SMT size or size of injury
            or identifying characteristic / SMS should be used in conjunction with this field. SUB
            is comprised of the following information items:</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:SubjectExistentialStatusCode" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:SubjectBodyStatusCode" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:SubjectBodyClassCode" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="SubjectPoseCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for a pose of a subject in an image</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="F">
            <xsd:annotation>
               <xsd:documentation>Full face frontal</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="R">
            <xsd:annotation>
               <xsd:documentation>Right profile (90 degree)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="L">
            <xsd:annotation>
               <xsd:documentation>Left profile (-90 degree)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="A">
            <xsd:annotation>
               <xsd:documentation>Angled pose</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="D">
            <xsd:annotation>
               <xsd:documentation>Determined 3D pose</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="SubjectPoseCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a pose of a subject in an image</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="biom:SubjectPoseCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:simpleType name="SubjectStatusCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>It contains a single letter describing the head of the team that
            processed the forensic data</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="X">
            <xsd:annotation>
               <xsd:documentation>Status of individual unknown</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="A">
            <xsd:annotation>
               <xsd:documentation>Data obtained from a living person (for unknown deceased =
                  antemortem)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="D">
            <xsd:annotation>
               <xsd:documentation>Data obtained from a non-living person
                  (deceased)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="SubjectStatusCodeType">
      <xsd:simpleContent>
         <xsd:extension base="biom:SubjectStatusCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:simpleType name="SystemNameCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>It contains the dental system codes</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="EDR">
            <xsd:annotation>
               <xsd:documentation>Electronic Dental Record System</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="FastID">
            <xsd:annotation>
               <xsd:documentation>Interface for completing the INTERPOL Disaster Victim
                  Identification forms</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="NamUS">
            <xsd:annotation>
               <xsd:documentation>The National Missing and Unidentified Persons
                  System</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="NCIC">
            <xsd:annotation>
               <xsd:documentation>The National Dental Image Repository of the National Crime
                  Information Center (NCIC) run by the Federal Bureau of Investigation
                  (FBI)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="PLASS">
            <xsd:annotation>
               <xsd:documentation>The DVI System International marketed by Plass Data Software
                  A/S</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="UDIM">
            <xsd:annotation>
               <xsd:documentation>The Unified Dental Identification Module (UDIM) of the Unified
                  Victim Identification System (UVIS)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="WinID">
            <xsd:annotation>
               <xsd:documentation>Dental Identification System</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Other">
            <xsd:annotation>
               <xsd:documentation>The coding system is not listed but is formally
                  documented</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="None">
            <xsd:annotation>
               <xsd:documentation>The ANSI/ADA Standard No. 1058 – Forensic Dental Codes are
                  selected and entered directly.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="SystemNameCodeType">
      <xsd:simpleContent>
         <xsd:extension base="biom:SystemNameCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>

   <!-- T -->

   <xsd:simpleType name="ThetaAngleMeasureSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for an angular position of a feature from another point in
