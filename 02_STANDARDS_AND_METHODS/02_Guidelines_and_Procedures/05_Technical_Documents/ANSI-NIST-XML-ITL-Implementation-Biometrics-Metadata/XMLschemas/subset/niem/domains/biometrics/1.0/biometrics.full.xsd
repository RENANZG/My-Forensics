         <xsd:documentation>It is mandatory if this field is used. It contains a single letter
            describing the head of the team that processed the forensic data </xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ForensicDentalCommentText" type="nc:TextType" nillable="true">
      <xsd:annotation>
         <xsd:documentation>A comment regarding a forensic dental sample</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ForensicDentalOralDetail" type="biom:ForensicDentalOralDetailType"
      nillable="true">
