            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="14">
            <xsd:annotation>
               <xsd:appinfo><i:Deprecated i:value="true"/></xsd:appinfo>
               <xsd:documentation>Latent palm photo. Deprecated.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="15">
            <xsd:annotation>
               <xsd:appinfo><i:Deprecated i:value="true"/></xsd:appinfo>
               <xsd:documentation>Latent palm lift. Deprecated.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="20">
            <xsd:annotation>
               <xsd:appinfo><i:Deprecated i:value="true"/></xsd:appinfo>
