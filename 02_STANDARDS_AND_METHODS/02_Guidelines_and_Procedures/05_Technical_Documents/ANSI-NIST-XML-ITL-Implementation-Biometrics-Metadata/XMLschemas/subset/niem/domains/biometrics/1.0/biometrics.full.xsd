         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="biom:LipPrintWidth" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:LipPrintHeight" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:PhiltrumWidth" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:PhiltrumHeight" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:UpperLipCharacterizationCodeList" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:LowerLipCharacterizationCodeList" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:LipContactLineDescriptorCode" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:LipPrintCharacterizationText" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:LipPrintPathologiesPeculiaritiesCodeList" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:LipPrintPathologiesPeculiaritiesText" minOccurs="0"
                  maxOccurs="unbounded"/>
               <xsd:element ref="biom:LipPrintSurfaceCodeList" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:LipPrintSurfaceText" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:LipPrintMediumCode" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:LipPrintMediumText" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:PersonFacialHairText" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="biom:LipPositionTensionDescriptiveText" minOccurs="0"
