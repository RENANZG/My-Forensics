<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:itl="http://biometrics.nist.gov/standard/2011"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	xmlns:s="http://niem.gov/niem/structures/2.0"
	xmlns:biom="http://niem.gov/niem/biometrics/1.0" 
	xmlns:i="http://niem.gov/niem/appinfo/2.0"
	xmlns:nc="http://niem.gov/niem/niem-core/2.0"
	xmlns:niem-xsd="http://niem.gov/niem/proxy/xsd/2.0"
	xmlns:fbi="http://niem.gov/niem/fbi/2.0"
	targetNamespace="http://biometrics.nist.gov/standard/2011" version="2015">
	<xsd:annotation>
		<xsd:documentation>ANSI/NIST-ITL 2011 Biometric Information Exchange
			Package</xsd:documentation>
		<xsd:appinfo>
			<i:ConformantIndicator>true</i:ConformantIndicator>
		</xsd:appinfo>
	</xsd:annotation>
	<xsd:import namespace="http://niem.gov/niem/appinfo/2.0"
		schemaLocation="../subset/niem/appinfo/2.0/appinfo.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/biometrics/1.0"
		schemaLocation="../subset/niem/domains/biometrics/1.0/biometrics.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/fbi/2.0" schemaLocation="../subset/niem/fbi/2.0/fbi.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/niem-core/2.0"
		schemaLocation="../subset/niem/niem-core/2.0/niem-core.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/proxy/xsd/2.0"
		schemaLocation="../subset/niem/proxy/xsd/2.0/xsd.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/structures/2.0"
		schemaLocation="../subset/niem/structures/2.0/structures.xsd"/>
	<xsd:element name="NISTBiometricInformationExchangePackage"
		type="itl:NISTBiometricInformationExchangePackageType">
		<xsd:annotation>
			<xsd:documentation>A biometric information exchange package representing one biometric
				transaction</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="NISTBiometricInformationExchangePackageType">
		<xsd:annotation>
			<xsd:documentation>A data type for a biometric information exchange package representing
				one biometric transaction</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ComplexObjectType">
				<xsd:sequence>
					<xsd:element ref="itl:PackageInformationRecord"/>
					<xsd:element ref="itl:PackageDescriptiveTextRecord" minOccurs="0"
						maxOccurs="unbounded"/>
					<xsd:element ref="itl:PackageDataRecord" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="PackageInformationRecord" type="itl:PackageInformationRecordType">
		<xsd:annotation>
			<xsd:documentation>A Type-1 transactional information record</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="PackageInformationRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for a Type-1 transactional information
				record</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ComplexObjectType">
				<xsd:sequence>
					<xsd:element ref="biom:RecordCategoryCode"/>
					<xsd:element ref="biom:Transaction"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="PackageDescriptiveTextRecord" type="itl:PackageDescriptiveTextRecordType">
		<xsd:annotation>
			<xsd:documentation>A Type-2 descriptive record</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="PackageDescriptiveTextRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for a Type-2 descriptive record</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="itl:PackageDataRecordType">
				<xsd:sequence>
					<xsd:element ref="itl:UserDefinedDescriptiveDetail" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="PackageDataRecord" abstract="true">
		<xsd:annotation>
			<xsd:documentation>A data concept for a biometric record; base element for record types
				4 through 99</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="PackageDataRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for a biometric record; base type for record types 4, 7,
				8 and 98</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ComplexObjectType">
				<xsd:sequence>
					<xsd:element ref="biom:RecordCategoryCode"/>
					<xsd:element ref="biom:ImageReferenceIdentification"/>
					<xsd:element ref="itl:UserDefinedFields" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CrossReferencePackageDataRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for a biometric record including Type 2 cross
				reference.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="itl:PackageDataRecordType">
				<xsd:sequence>
					<xsd:element ref="biom:Type2CrossReferenceIdentification" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="MultipleCrossReferencePackageDataRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for a biometric record including Type 2, 10 and 22 cross
				references</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="itl:PackageDataRecordType">
				<xsd:sequence>
					<xsd:element ref="biom:Type2CrossReferenceIdentification" minOccurs="0"/>
					<xsd:element ref="biom:Type10CrossReferenceIdentification" minOccurs="0"
						maxOccurs="99"/>
					<xsd:element ref="biom:Type22CrossReferenceIdentification" minOccurs="0"
						maxOccurs="99"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ExtendedPackageDataRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for an extended biometric record; base type for record
				types 9,13-17,19,20,21, and 99</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="itl:PackageDataRecordType">
				<xsd:sequence>
					<xsd:element ref="biom:ProcessAnnotation" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="biom:AssociatedContext" minOccurs="0" maxOccurs="255"/>
					<xsd:element ref="biom:ImageHashValue" minOccurs="0"/>
					<xsd:element ref="biom:SourceRepresentation" minOccurs="0" maxOccurs="255"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CrossReferenceExtendedPackageDataRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for an extended biometric record including Type 2 cross
				reference; base type for record types 10, 18 and 22</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="itl:CrossReferencePackageDataRecordType">
				<xsd:sequence>
					<xsd:element ref="biom:ProcessAnnotation" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="biom:AssociatedContext" minOccurs="0" maxOccurs="255"/>
					<xsd:element ref="biom:ImageHashValue" minOccurs="0"/>
					<xsd:element ref="biom:SourceRepresentation" minOccurs="0" maxOccurs="255"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ExtendedAudioPackageDataRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for an extended biometric record for base type for record
				type 11</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="itl:PackageDataRecordType">
				<xsd:sequence>
					<xsd:element ref="biom:ProcessAnnotation" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="biom:AssociatedContext" minOccurs="0" maxOccurs="255"/>
					<xsd:element ref="biom:DigitalAudioRecordHashValue" minOccurs="0"/>
					<xsd:element ref="biom:SourceRepresentation" minOccurs="0" maxOccurs="255"/>
					<xsd:element ref="nc:BinaryBase64Object" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="MultipleCrossReferenceExtendedPackageDataRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for an extended biometric record including Type 2, 10 and
				22 cross reference; base type for record type 12</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="itl:MultipleCrossReferencePackageDataRecordType">
				<xsd:sequence>
					<xsd:element ref="biom:ProcessAnnotation" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="biom:AssociatedContext" minOccurs="0" maxOccurs="255"/>
					<xsd:element ref="biom:ImageHashValue" minOccurs="0"/>
					<xsd:element ref="nc:BinaryBase64Object" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="PackageHighResolutionGrayscaleImageRecord"
		type="itl:PackageFingerprintImageRecordType" substitutionGroup="itl:PackageDataRecord">
		<xsd:annotation>
			<xsd:documentation>A Type-4 high-resolution grayscale fingerprint image
				record</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="PackageFingerprintImageRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for a fingerprint image record
				(Type-4)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="itl:PackageDataRecordType">
				<xsd:sequence>
					<xsd:element ref="biom:FingerprintImage"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="PackageUserDefinedImageRecord" type="itl:PackageUserDefinedImageRecordType"
		substitutionGroup="itl:PackageDataRecord">
		<xsd:annotation>
			<xsd:documentation>A Type-7 user-defined image record</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="PackageUserDefinedImageRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for a user-defined image record
				(Type-7)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="itl:PackageDataRecordType">
				<xsd:sequence>
					<xsd:element ref="biom:BiometricImage" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="PackageSignatureImageRecord" type="itl:PackageSignatureImageRecordType"
		substitutionGroup="itl:PackageDataRecord">
		<xsd:annotation>
			<xsd:documentation>A Type-8 signature image record</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="PackageSignatureImageRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for a signature image record (Type-8)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="itl:PackageDataRecordType">
				<xsd:sequence>
					<xsd:element ref="biom:SignatureImage"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="PackageMinutiaeRecord" type="itl:PackageMinutiaeRecordType"
		substitutionGroup="itl:PackageDataRecord">
		<xsd:annotation>
			<xsd:documentation>A Type-9 minutiae data record</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="PackageMinutiaeRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for a minutiae data record (Type-9)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="itl:ExtendedPackageDataRecordType">
				<xsd:sequence>
					<xsd:element ref="biom:MinutiaeImpressionCaptureCategory"/>
					<xsd:element ref="biom:MinutiaeFormatNISTStandardIndicator"/>
					<xsd:element ref="biom:MinutiaeUniversalLatentWorkstationAnnotationText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="biom:BiometricCaptureDetail" minOccurs="0"/>
					<xsd:element ref="biom:RecordMinutiae" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="PackageFacialAndSMTImageRecord" type="itl:PackageFacialAndSMTImageRecordType"
		substitutionGroup="itl:PackageDataRecord">
		<xsd:annotation>
			<xsd:documentation>A Type-10 facial or SMT image record</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="PackageFacialAndSMTImageRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for a facial or SMT image record
				(Type-10)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="itl:CrossReferenceExtendedPackageDataRecordType">
				<xsd:choice>
					<xsd:element ref="biom:FaceImage"/>
					<xsd:element ref="biom:PhysicalFeatureImage"/>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:simpleType name="SMTCodeType">
		<xsd:annotation>
			<xsd:documentation>A union to add new SMT values to the fbi codes</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="itl:SMTDescriptionCodeExtensionSimpleType fbi:SMTCodeSimpleType"/>
	</xsd:simpleType>
	<xsd:simpleType name="SMTDescriptionCodeExtensionSimpleType">
		<xsd:annotation>
			<xsd:documentation>A code list of new SMT codes</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="UTAT ABDM">
				<xsd:annotation>
					<xsd:documentation>Abdomen</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT LANKL">
				<xsd:annotation>
					<xsd:documentation>Ankle, left</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT ANKL">
				<xsd:annotation>
					<xsd:documentation>Ankle, nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT RANKL">
				<xsd:annotation>
					<xsd:documentation>Ankle, right</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT L ARM">
				<xsd:annotation>
					<xsd:documentation>Arm, left</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT ULAR">
				<xsd:annotation>
					<xsd:documentation>Arm, left upper</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT ARM">
				<xsd:annotation>
					<xsd:documentation>Arm, nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT R ARM">
				<xsd:annotation>
					<xsd:documentation>Arm, right</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT URARM">
				<xsd:annotation>
					<xsd:documentation>Arm, right upper</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT BACK">
				<xsd:annotation>
					<xsd:documentation>Back</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT LBRST">
				<xsd:annotation>
					<xsd:documentation>Breast, left</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT BRST">
				<xsd:annotation>
					<xsd:documentation>Breast, nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT RBRST">
				<xsd:annotation>
					<xsd:documentation>Breast, right</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT LBUTK">
				<xsd:annotation>
					<xsd:documentation>Buttock, left</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT RBUTK">
				<xsd:annotation>
					<xsd:documentation>Buttock, right</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT BUTTK">
				<xsd:annotation>
					<xsd:documentation>Buttocks, nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT LCALF">
				<xsd:annotation>
					<xsd:documentation>Calf, left</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT CALF">
				<xsd:annotation>
					<xsd:documentation>Calf, nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT RCALF">
				<xsd:annotation>
					<xsd:documentation>Calf, right</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT L CHK">
				<xsd:annotation>
					<xsd:documentation>Cheek (face), left</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT CHEEK">
				<xsd:annotation>
					<xsd:documentation>Cheek (face), nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT R CHK">
				<xsd:annotation>
					<xsd:documentation>Cheek (face), right</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT CHEST">
				<xsd:annotation>
					<xsd:documentation>Chest</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT CHIN">
				<xsd:annotation>
					<xsd:documentation>Chin</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT L EAR">
				<xsd:annotation>
					<xsd:documentation>Ear, left</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT EAR">
				<xsd:annotation>
					<xsd:documentation>Ear, nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT R EAR">
				<xsd:annotation>
					<xsd:documentation>Ear, right</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT L ELB">
				<xsd:annotation>
					<xsd:documentation>Elbow, left</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT ELBOW">
				<xsd:annotation>
					<xsd:documentation>Elbow, nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT R ELB">
				<xsd:annotation>
					<xsd:documentation>Elbow, right</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT FACE">
				<xsd:annotation>
					<xsd:documentation>Face, nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT L FGR">
				<xsd:annotation>
					<xsd:documentation>Finger(s), left hand</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT FNGR">
				<xsd:annotation>
					<xsd:documentation>Finger, nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT R FGR">
				<xsd:annotation>
					<xsd:documentation>Finger(s), right hand</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT LFOOT">
				<xsd:annotation>
					<xsd:documentation>Foot, left</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT FOOT">
				<xsd:annotation>
					<xsd:documentation>Foot, nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT RFOOT">
				<xsd:annotation>
					<xsd:documentation>Foot, right</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT LFARM">
				<xsd:annotation>
					<xsd:documentation>Forearm, left</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT FARM">
				<xsd:annotation>
					<xsd:documentation>Forearm, nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT RFARM">
				<xsd:annotation>
					<xsd:documentation>Forearm, right</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT FHD">
				<xsd:annotation>
					<xsd:documentation>Forehead</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT FLBOD">
				<xsd:annotation>
					<xsd:documentation>Full body</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT GROIN">
				<xsd:annotation>
					<xsd:documentation>Groin area</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT L HND">
				<xsd:annotation>
					<xsd:documentation>Hand, left</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT HAND">
				<xsd:annotation>
					<xsd:documentation>Hand, nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT R HND">
				<xsd:annotation>
					<xsd:documentation>Hand, right</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT HEAD">
				<xsd:annotation>
					<xsd:documentation>Head, nonspecific (use the MIS Field to further describe
						location)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT L HIP">
				<xsd:annotation>
					<xsd:documentation>Hip, left</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT HIP">
				<xsd:annotation>
					<xsd:documentation>Hip, nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT R HIP">
				<xsd:annotation>
					<xsd:documentation>Hip, right</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT LKNEE">
				<xsd:annotation>
					<xsd:documentation>Knee, left</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT KNEE">
				<xsd:annotation>
					<xsd:documentation>Knee, nonspecific </xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT RKNEE">
				<xsd:annotation>
					<xsd:documentation>Knee, right </xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT L LEG">
				<xsd:annotation>
					<xsd:documentation>Leg, left</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UTAT LEG">
				<xsd:annotation>
					<xsd:documentation>Leg, nonspecific</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
