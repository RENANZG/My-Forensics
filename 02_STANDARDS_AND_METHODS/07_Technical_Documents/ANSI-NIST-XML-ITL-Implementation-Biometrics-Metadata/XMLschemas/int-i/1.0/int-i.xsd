<?xml version="1.0"?>
<xsd:schema xmlns:int-i="http://interpol.gov/int-i/1.0"
            xmlns:s="http://niem.gov/niem/structures/2.0"
            xmlns:itl="http://biometrics.nist.gov/standard/2011"
            xmlns:biom="http://niem.gov/niem/biometrics/1.0"
            xmlns:nc="http://niem.gov/niem/niem-core/2.0"
            xmlns:i="http://niem.gov/niem/appinfo/2.0"
            xmlns:niem-xsd="http://niem.gov/niem/proxy/xsd/2.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:iso3166="http://niem.gov/niem/iso_3166/2.0"
            targetNamespace="http://interpol.gov/int-i/1.0" version="2.0">
    <xsd:import namespace="http://niem.gov/niem/structures/2.0"
                schemaLocation="../../../XMLschemas/subset/niem/structures/2.0/structures.xsd">
        <xsd:annotation>
            <xsd:documentation>NIEM Defined structures for creating types</xsd:documentation>
        </xsd:annotation>
    </xsd:import>
    <xsd:import namespace="http://niem.gov/niem/appinfo/2.0"
                schemaLocation="../../../XMLschemas/subset/niem/appinfo/2.0/appinfo.xsd">
        <xsd:annotation>
            <xsd:documentation>NIEM Base Application Information for creating types</xsd:documentation>
        </xsd:annotation>
    </xsd:import>
    <xsd:import namespace="http://niem.gov/niem/biometrics/1.0"
                schemaLocation="../../../XMLschemas/subset/niem/domains/biometrics/1.0/biometrics.xsd">
        <xsd:annotation>
            <xsd:documentation>ANSI/NIST Biometrics Domain Schema</xsd:documentation>
        </xsd:annotation>
    </xsd:import>
    <xsd:import namespace="http://niem.gov/niem/niem-core/2.0"
                schemaLocation="../../../XMLschemas/subset/niem/niem-core/2.0/niem-core.xsd">
        <xsd:annotation>
            <xsd:documentation>NIEM Core Data Model</xsd:documentation>
        </xsd:annotation>
    </xsd:import>
    <xsd:import namespace="http://niem.gov/niem/proxy/xsd/2.0"
                schemaLocation="../../../XMLschemas/subset/niem/proxy/xsd/2.0/xsd.xsd">
        <xsd:annotation>
            <xsd:documentation>NIEM XML Proxy types</xsd:documentation>
        </xsd:annotation>
    </xsd:import>
    <xsd:import namespace="http://niem.gov/niem/iso_3166/2.0"
                schemaLocation="../../../XMLschemas/subset/niem/iso_3166/2.0/iso_3166.xsd">
        <xsd:annotation>
            <xsd:documentation>ISO 3166 Country Codes</xsd:documentation>
        </xsd:annotation>
    </xsd:import>
    <xsd:import namespace="http://biometrics.nist.gov/standard/2011"
                schemaLocation="../../../XMLschemas/exchange/itl.xsd">
        <xsd:annotation>
            <xsd:documentation>ANSI/NIST-ITL 2011 Biometric Information Exchange Package</xsd:documentation>
        </xsd:annotation>
    </xsd:import>
    <xsd:complexType name="NationalIdentifierType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="int-i:LocationCountryISO3166Alpha3Code" minOccurs="1"/>
                    <xsd:element ref="nc:IdentificationID" minOccurs="1"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ContactInformationType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="nc:ContactTelephoneNumber" minOccurs="0"/>
                    <xsd:element ref="nc:OrganizationName" minOccurs="0"/>
                    <xsd:element ref="nc:PersonFullName" minOccurs="0"/>
                    <xsd:element ref="int-i:AdditionalContactInformationText" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="CaptureOrganizationType">
        <xsd:complexContent>
            <xsd:extension base="nc:OrganizationType">
                <xsd:sequence>
                    <xsd:element ref="nc:LocationAddress" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="CustomDefinedDescriptiveDetailType">
        <xsd:annotation>
            <xsd:appinfo>
                <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
            </xsd:appinfo>
            <xsd:documentation>Custom defined fields, which are not part of INTERPOL implementation</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:any processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="DomainDefinedDescriptiveDetailType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="int-i:RecordActivity" minOccurs="0"/>
                    <xsd:element ref="int-i:RecordSubject" minOccurs="0"/>
                    <xsd:element ref="int-i:RecordOffence" minOccurs="0"/>
                    <xsd:element ref="int-i:RecordCivil" minOccurs="0"/>
                    <xsd:element ref="int-i:RecordMetadata" minOccurs="0"/>
                    <xsd:element ref="int-i:TransactionResponseData" minOccurs="0"/>
                    <xsd:element ref="int-i:TransactionMetadata" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ErrorMessageType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="int-i:ErrorCode" minOccurs="0"/>
                    <xsd:element ref="int-i:ErrorFieldMnemonicCode" minOccurs="0"/>
                    <xsd:element ref="int-i:ErrorMessageText" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="PersonNameType">
        <xsd:complexContent>
            <xsd:extension base="nc:PersonNameType">
                <xsd:sequence>
                    <xsd:element ref="int-i:PersonExtendedName" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="PersonAliasesType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="int-i:PersonName" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="MiscellaneousIdentificationIDXType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="int-i:MiscIDType" minOccurs="0"/>
                    <xsd:element ref="nc:IdentificationID" minOccurs="1"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="MiscellaneousIdentificationType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="int-i:InterpolReferenceNumber" minOccurs="0"/>
                    <xsd:element ref="int-i:NationalPersonIdentifier" minOccurs="0"/>
                    <xsd:element ref="int-i:NationalPersonInquiryIdentifier" minOccurs="0"/>
                    <xsd:element ref="int-i:NationalCaseIdentifier" minOccurs="0"/>
                    <xsd:element ref="int-i:LatentCaseEvidenceIdentifier" minOccurs="0"/>
                    <xsd:element ref="int-i:LatentIdentifier" minOccurs="0"/>
                    <xsd:element ref="int-i:LatentUniqueIdentifier" minOccurs="0"/>
                    <xsd:element ref="int-i:NationalBusinessReference" minOccurs="0"/>
                    <xsd:element ref="int-i:MiscellaneousIdentificationID1" minOccurs="0"/>
                    <xsd:element ref="int-i:MiscellaneousIdentificationID2" minOccurs="0"/>
                    <xsd:element ref="int-i:MiscellaneousIdentificationID3" minOccurs="0"/>
                    <xsd:element ref="int-i:MiscellaneousIdentificationID4" minOccurs="0"/>
                    <xsd:element ref="int-i:MiscellaneousIdentificationID5" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="RecordActivityType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="biom:RecordCreationDate" minOccurs="0"/>
                    <xsd:element ref="int-i:CaptureOrganization" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="RecordMetadataType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="int-i:MiscellaneousIdentification" minOccurs="0"/>
                    <xsd:element ref="int-i:ContactInformation" minOccurs="0" maxOccurs="unbounded"/>
                    <xsd:element ref="nc:ExpirationDate" minOccurs="0"/>
                    <xsd:element ref="int-i:RecordCreationDate" minOccurs="0"/>
                    <xsd:element ref="int-i:RecordLastUpdateDate" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="RecordSubjectType">
        <xsd:complexContent>
            <xsd:extension base="nc:PersonType">
                <xsd:sequence>
                    <xsd:element ref="int-i:PersonAliases" minOccurs="0"/>
                    <xsd:element ref="int-i:PersonBirthDateRange" minOccurs="0"/>
                    <xsd:element ref="int-i:PersonSexCode" minOccurs="0"/>
                    <xsd:element ref="int-i:PersonFather" minOccurs="0"/>
                    <xsd:element ref="int-i:PersonMother" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="RecordOffenceType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="int-i:OffenceDescriptionCode" minOccurs="0"/>
                    <xsd:element ref="int-i:OffenceDescriptionText" minOccurs="0"/>
                    <xsd:element ref="int-i:ArrestLocation" minOccurs="0"/>
                    <xsd:choice>
                        <xsd:element ref="int-i:OffenceDateTime" minOccurs="0"/>
                        <xsd:element ref="int-i:OffenceDateTimeRange" minOccurs="0"/>
                    </xsd:choice>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="RecordCivilType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="int-i:CivilDescriptionCode" minOccurs="0"/>
                    <xsd:element ref="int-i:CivilDescriptionText" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="RespondentsListType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="int-i:TransactionCandidate"/>
                    <xsd:element ref="int-i:TransactionCandidatesTotalQuantity"/>
                    <xsd:element ref="int-i:TransactionCategoryCode"/>
                    <xsd:element ref="int-i:TransactionSearchResultsCode"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="TransactionCandidateType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="int-i:CandidateSequenceID"/>
                    <xsd:element ref="int-i:TransactionCandidateManualVerificationIndicator"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="TransactionMetadataType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="int-i:ResultDeterminationMode" minOccurs="0" maxOccurs="1"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="TransactionResponseDataType">
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="int-i:ErrorMessage" minOccurs="0" maxOccurs="unbounded"/>
                    <xsd:element ref="int-i:RespondentsList" minOccurs="0"/>
                    <xsd:element ref="int-i:LatentMatchingPosition" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="TransactionCategoryCodeType">
        <xsd:annotation>
            <xsd:documentation>A data type for an extension of the biom:TransactionType to add the type of transaction
                defined by receiving agency.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="int-i:TransactionCategoryCodeSimpleType">
                <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:complexType name="AbstractTransactionOrganizationType" abstract="true">
        <xsd:annotation>
            <xsd:documentation>A data type for a restriction of nc:OrganizationType to allow to
                make the OrganizationName mandatory.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:restriction base="nc:OrganizationType">
                <xsd:sequence>
                    <xsd:element ref="nc:OrganizationIdentification" minOccurs="0" maxOccurs="1"/>
                    <xsd:element ref="nc:OrganizationName" minOccurs="1" maxOccurs="1"/>
                </xsd:sequence>
            </xsd:restriction>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="TransactionOrganizationType">
        <xsd:annotation>
            <xsd:documentation>A data type for an extension of int:AbstractTransactionOrganisation to allow to
                specify the 3-letter country code of the agency.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="int-i:AbstractTransactionOrganizationType">
                    <xsd:sequence>
                        <xsd:element ref="int-i:LocationCountryISO3166Alpha3Code" minOccurs="1" maxOccurs="1"/>
                    </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="LatentMatchingPositionType">
        <xsd:annotation>
            <xsd:documentation></xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="int-i:LatentMatchingPositionSearch" minOccurs="1" maxOccurs="1"/>
                    <xsd:element ref="int-i:LatentMatchingPositionCandidate" minOccurs="1" maxOccurs="1"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <!--****************************************-->
    <!--****************************************-->
    <!--****************************************-->
    <!-- Int-I Element Declarations-->
    <!--****************************************-->
    <!--****************************************-->
    <!--****************************************-->
    <!-- Type 1 elements -->
    <xsd:element name="TransactionCategoryCode" type="int-i:TransactionCategoryCodeType"
                 substitutionGroup="biom:TransactionCategory"/>
    <xsd:element name="TransactionDestinationOrganization" type="int-i:TransactionOrganizationType"
                 substitutionGroup="biom:TransactionDestinationOrganization"/>
    <xsd:element name="TransactionOriginatingOrganization" type="int-i:TransactionOrganizationType"
                 substitutionGroup="biom:TransactionOriginatingOrganization"/>
    <xsd:element name="LocationCountryISO3166Alpha3Code" type="int-i:ISO3166Alpha3CodeType"/>
    <!-- Type 2 elements -->
    <xsd:element name="AdditionalContactInformationText" type="nc:TextType"/>
    <xsd:element name="ArrestLocation" type="nc:LocationType"/>
    <xsd:element name="CountryCaseReference" type="nc:TextType"/>
    <xsd:element name="CaptureOrganization" type="int-i:CaptureOrganizationType"/>
    <xsd:element name="CandidateSequenceID" type="nc:IdentificationType"/>
    <xsd:element name="CivilDescriptionCode" type="int-i:CivilDescriptionCodeType"/>
    <xsd:element name="CivilDescriptionText" type="nc:TextType"/>
    <xsd:element name="ContactInformation" type="int-i:ContactInformationType"/>
    <xsd:element name="CountrySearchedIndicator" type="niem-xsd:boolean"/>
    <xsd:element name="CustomDefinedDescriptiveDetail" type="int-i:CustomDefinedDescriptiveDetailType"
                 substitutionGroup="itl:OtherDescriptiveDetail"/>
    <xsd:element name="DomainDefinedDescriptiveDetail" type="int-i:DomainDefinedDescriptiveDetailType"
                 substitutionGroup="itl:DomainDefinedDescriptiveDetail"/>
    <xsd:element name="ErrorCode" type="int-i:ErrorCodeType"/>
    <xsd:element name="ErrorFieldMnemonicCode" type="nc:TextType"/>
    <xsd:element name="ErrorMessage" type="int-i:ErrorMessageType"/>
    <xsd:element name="ErrorMessageText" type="nc:TextType"/>
    <xsd:element name="InterpolReferenceNumber" type="nc:TextType"/>
    <xsd:element name="NationalBusinessReference" type="int-i:NationalIdentifierType"/>
    <xsd:element name="NationalPersonIdentifier" type="int-i:NationalIdentifierType"/>
    <xsd:element name="NationalPersonInquiryIdentifier" type="int-i:NationalIdentifierType"/>
    <xsd:element name="NationalCaseIdentifier" type="int-i:NationalIdentifierType"/>
    <xsd:element name="LatentMatchingPosition" type="int-i:LatentMatchingPositionType"/>
    <xsd:element name="LatentMatchingPositionSearch" type="int-i:LatentMatchingPositionSimpleType"/>
    <xsd:element name="LatentMatchingPositionCandidate" type="int-i:LatentMatchingPositionSimpleType"/>
    <xsd:element name="LatentCaseEvidenceIdentifier" type="nc:TextType"/>
    <xsd:element name="LatentIdentifier" type="nc:TextType"/>
    <xsd:element name="LatentUniqueIdentifier" type="nc:TextType"/>
    <xsd:element name="MiscellaneousIdentificationID1" type="int-i:MiscellaneousIdentificationIDXType"/>
    <xsd:element name="MiscellaneousIdentificationID2" type="int-i:MiscellaneousIdentificationIDXType"/>
    <xsd:element name="MiscellaneousIdentificationID3" type="int-i:MiscellaneousIdentificationIDXType"/>
    <xsd:element name="MiscellaneousIdentificationID4" type="int-i:MiscellaneousIdentificationIDXType"/>
    <xsd:element name="MiscellaneousIdentificationID5" type="int-i:MiscellaneousIdentificationIDXType"/>
    <xsd:element name="MiscIDType" type="nc:TextType"/>
    <xsd:element name="OffenceDescriptionText" type="nc:TextType"/>
    <xsd:element name="OffenceDateTime" type="nc:DateType"/>
    <xsd:element name="OffenceDateTimeRange" type="nc:DateRangeType"/>
    <xsd:element name="PersonBirthDateRange" type="nc:DateRangeType"/>
    <xsd:element name="PersonPlaceOfBirth" type="nc:LocationType"/>
    <xsd:element name="PersonExtendedName" type="nc:TextType" nillable="true">
        <xsd:annotation>
            <xsd:documentation>A name of a person for cultures that do not follow the First,Middle,Last naming
                convention.
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PersonSexCode" type="int-i:PersonSexCodeType"/>
    <xsd:element name="PersonFather" type="int-i:PersonNameType"/>
    <xsd:element name="PersonAliases" type="int-i:PersonAliasesType"/>
    <xsd:element name="PersonMother" type="int-i:PersonNameType"/>
    <xsd:element name="PersonName" type="int-i:PersonNameType" substitutionGroup="nc:PersonName"/>
    <xsd:element name="MiscellaneousIdentification" type="int-i:MiscellaneousIdentificationType"/>
    <xsd:element name="OffenceDescriptionCode" type="int-i:OffenceDescriptionCodeType"/>
    <xsd:element name="RecordActivity" type="int-i:RecordActivityType"/>
    <xsd:element name="RecordCreationDate" type="nc:DateType"/>
    <xsd:element name="RecordLastUpdateDate" type="nc:DateType"/>
    <xsd:element name="RecordMetadata" type="int-i:RecordMetadataType"/>
    <xsd:element name="RecordOffence" type="int-i:RecordOffenceType"/>
    <xsd:element name="RecordCivil" type="int-i:RecordCivilType"/>
    <xsd:element name="RecordSubject" type="int-i:RecordSubjectType"/>
    <xsd:element name="RespondentsList" type="int-i:RespondentsListType"/>
    <xsd:element name="ResultDeterminationMode" type="int-i:ResultDeterminationModeSimpleType"/>
    <xsd:element name="TransactionCandidate" type="int-i:TransactionCandidateType"/>
    <xsd:element name="TransactionCandidateManualVerificationIndicator" type="niem-xsd:boolean"/>
    <xsd:element name="TransactionCandidatesTotalQuantity" type="niem-xsd:nonNegativeInteger"/>
    <xsd:element name="TransactionMetadata" type="int-i:TransactionMetadataType"/>
    <xsd:element name="TransactionResponseData" type="int-i:TransactionResponseDataType"/>
    <xsd:element name="TransactionSearchResultsCode" type="int-i:TransactionSearchResultsCodeType"/>
    <!-- ====== Simple Types =============== -->
    <xsd:simpleType name="ErrorCodeType">
        <xsd:restriction base="xsd:token">
            <xsd:enumeration value="001">
                <xsd:annotation>
                    <xsd:documentation>ERROR RECORD NOT FOUND</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="002">
                <xsd:annotation>
                    <xsd:documentation>ERROR RECORD ALREADY EXISTS</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="003">
                <xsd:annotation>
                    <xsd:documentation>ERROR UNAUTHORISED ACCESS</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="101">
                <xsd:annotation>
                    <xsd:documentation>MANDATORY FIELD MISSING</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="102">
                <xsd:annotation>
                    <xsd:documentation>INVALID RECORD TYPE</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="103">
                <xsd:annotation>
                    <xsd:documentation>UNDEFINED FIELD</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="104">
                <xsd:annotation>
                    <xsd:documentation>EXCEED THE MAXIMUM OCCURRENCE</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="105">
                <xsd:annotation>
                    <xsd:documentation>INVALID NUMBER OF SUBFIELDS</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="106">
                <xsd:annotation>
                    <xsd:documentation>FIELD LENGTH TOO SHORT</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="107">
                <xsd:annotation>
                    <xsd:documentation>FIELD LENGTH TOO LONG</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="108">
                <xsd:annotation>
                    <xsd:documentation>FIELD IS NOT A NUMBER AS EXPECTED</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="109">
                <xsd:annotation>
                    <xsd:documentation>FIELD NUMBER VALUE TOO SMALL</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="110">
                <xsd:annotation>
                    <xsd:documentation>FIELD NUMBER VALUE TOO BIG</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="111">
                <xsd:annotation>
                    <xsd:documentation>INVALID CHARACTER</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="112">
                <xsd:annotation>
                    <xsd:documentation>INVALID DATE</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="113">
                <xsd:annotation>
                    <xsd:documentation>INVALID TIME</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="114">
                <xsd:annotation>
                    <xsd:documentation>INVALID DATE OR TIME</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="115">
                <xsd:annotation>
                    <xsd:documentation>INVALID ITEM VALUE</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="116">
                <xsd:annotation>
                    <xsd:documentation>INVALID TYPE OF TRANSACTION</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="117">
                <xsd:annotation>
                    <xsd:documentation>INVALID RECORD DATA</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="201">
                <xsd:annotation>
                    <xsd:documentation>ERROR INVALID TCN</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="501">
                <xsd:annotation>
                    <xsd:documentation>ERROR INSUFFICIENT FINGERPRINT QUALITY</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="502">
                <xsd:annotation>
                    <xsd:documentation>ERROR MISSING FINGERPRINTS</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="503">
                <xsd:annotation>
                    <xsd:documentation>ERROR FINGERPRINT SEQUENCE CHECK FAILED</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="999">
                <xsd:annotation>
                    <xsd:documentation>ERROR ANY OTHER ERROR. FOR FURTHER DETAILS CALL DESTINATION AGENCY
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="CivilDescriptionCodeType">
        <xsd:restriction base="xsd:token">
            <xsd:enumeration value="AMN">
                <xsd:annotation>
                    <xsd:documentation>AMNESIA</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
